{
  "name": "rhizones",
  "version": "3.0.0",
  "description": "How to use create-react-app with a custom Node API on Heroku",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "start": "cd the_rhizone/ && concurrently \"npm start\" \"node server\"",
    "build": "cd the_rhizone/ && npm install && npm run build",
    "postinstall": "&& npm start"
  },
  "cacheDirectories": [
    "node_modules",
    "react-ui/node_modules"
  ],
  "dependencies": {
    "concurrently": "^5.1.0",
    "express": "^4.16.4"
  },
  "keywords": [
    "node",
    "heroku",
    "create-react-app",
    "react"
  ],
  "license": "MIT",
  "devDependencies": {}
}
