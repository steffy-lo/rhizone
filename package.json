{
  "name": "rhizones",
  "version": "3.0.0",
  "description": "How to use create-react-app with a custom Node API on Heroku",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
        "setup": "cd the_rhizone && npm install && cd ..",
        "build-run": "cd the_rhizone && npm run build && node server.js",
        "start": "node the_rhizone/server.js",
        "heroku-postbuild": "cd the_rhizone && npm install && npm run build",
        "test": "echo \"Error: no test specified\" && exit 1"
	},
  "cacheDirectories": [
    "node_modules",
    "react-ui/node_modules"
  ],
  "dependencies": {
    "concurrently": "^5.1.0",
    "express": "^4.16.4"
  },
  "keywords": [
    "node",
    "heroku",
    "create-react-app",
    "react"
  ],
  "license": "MIT",
  "devDependencies": {}
}
